<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="OpenLibrary Regression">
    <parameter name="BaseURL" value="https://openlibrary.org/account/login"/>

    <test name="Regression">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>

        <packages>
            <package name="tests"/>
        </packages>
    </test>
</suite>
